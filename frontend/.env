# API base URL for backend FastAPI server
REACT_APP_API_BASE_URL=http://localhost:8000

# WebSocket URL for real-time chat
REACT_APP_WS_URL=ws://localhost:8000/ws

# OpenAI key if used in frontend (not recommended, better to handle in backend)
# REACT_APP_OPENAI_API_KEY=your-openai-api-key

# Google OAuth client ID (if using OAuth login from frontend)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth App ID (if needed)
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
